{
  "name": "Technology Company Leadership - Test ICP",
  "icp_text": "Large technology companies and e-commerce platforms seeking executive leadership contacts for business development and partnerships.",
  "queries": [
    "technology company executive leadership contacts"
  ],
  "specific_urls": ["https://www.amazon.com/"],
  "required_fields": {
    "company": ["name", "hq_location", "number_of_locations", "ownership_type", "industry"],
    "contact": ["email", "job_title", "full_name"]
  },
  "optional_fields": {
    "company": ["tech_stack", "revenue_range", "employee_count", "specialties", "founded_year", "company_type"],
    "contact": ["phone", "linkedin_url", "decision_maker", "department", "seniority"]
  },
  "threshold": 0.5,
  "filtering_strict": false,
  "mode": "fast",
  "refresh_policy": {
    "revisit_after_days": 90,
    "failure_revisit_days": 7,
    "domain_serp_ttl_hours": 48,
    "crawl_ttl_days": 14,
    "enrich_ttl_days": 30
  },
  "concurrency": {
    "max_concurrent_requests": 3
  },
  "search": {
    "max_pages": 5,
    "max_results_per_query": 50
  },
  "testing": {
    "process_rejected": true,
    "process_low_crawl": true,
    "force_contact_discovery": true
  },
  "caps": {
    "company_usd": 0.02,
    "contact_usd": 0.03,
    "run_usd_max": 10.0,
    "max_domains_per_run": 20,
    "max_crawl_per_run": 20,
    "max_enrich_per_run": 20
  },
  "role_priority": {
    "ceo": 1,
    "founder": 1,
    "cto": 1,
    "president": 1,
    "vp engineering": 2,
    "vp business development": 2,
    "director": 3,
    "manager": 4,
    "default": 99
  },
  "exports": {
    "enabled": true
  },
  "default_country": "US",
  "exclusion_criteria": {
    "ownership_types": [],
    "max_locations": 10000,
    "excluded_keywords": []
  },
  "targeting": {
    "location_types": ["global", "headquarters"],
    "company_sizes": ["enterprise", "large"],
    "ownership_models": ["public", "private"],
    "geographic_focus": ["North America", "United States", "Global"]
  },
  "intent_config": {
    "purpose": "technology_partnership",
    "target_action": "business_development",
    "intent_signals": {
      "high_intent": [
        "seeking partnerships",
        "business development opportunities",
        "strategic alliances",
        "partner program",
        "collaboration opportunities",
        "innovation partnerships",
        "technology partnerships",
        "API partnerships",
        "platform integrations"
      ],
      "medium_intent": [
        "expanding services",
        "new markets",
        "growth initiatives",
        "product development",
        "innovation focus",
        "technology adoption"
      ],
      "low_intent": [
        "not accepting partners",
        "closed ecosystem",
        "no external collaborations"
      ]
    },
    "intent_sources": [
      "about_pages",
      "leadership_pages",
      "careers_pages",
      "news_pages",
      "team_pages",
      "contact_pages",
      "partners_pages",
      "press_releases"
    ],
    "scoring_rules": {
      "high_intent_weight": 0.8,
      "medium_intent_weight": 0.4,
      "low_intent_weight": -0.3,
      "base_score": 0.5,
      "location_bonus": 0.1,
      "size_bonus": 0.1,
      "leadership_bonus": 0.1
    }
  }
}
