{
  "name": "Digital Marketing Agency - Test ICP",
  "icp_text": "Digital marketing agencies and tech startups seeking executive leadership contacts for partnerships and business development.",
  "queries": [
    "digital marketing agency executive contacts",
    "tech startup founder contacts"
  ],
  "specific_urls": ["https://www.octivdigital.com/about"],
  "required_fields": {
    "company": ["name", "industry"],
    "contact": ["job_title", "full_name"]
  },
  "optional_fields": {
    "company": ["hq_location", "revenue_range", "employee_count", "specialties", "founded_year", "company_type", "number_of_locations", "ownership_type"],
    "contact": ["email", "phone", "linkedin_url", "decision_maker", "department", "seniority"]
  },
  "threshold": 0.3,
  "filtering_strict": false,
  "mode": "fast",
  "refresh_policy": {
    "revisit_after_days": 90,
    "failure_revisit_days": 7,
    "domain_serp_ttl_hours": 48,
    "crawl_ttl_days": 14,
    "enrich_ttl_days": 30
  },
  "concurrency": {
    "max_concurrent_requests": 3
  },
  "search": {
    "max_pages": 5,
    "max_results_per_query": 50
  },
  "testing": {
    "process_rejected": true,
    "process_low_crawl": true,
    "force_contact_discovery": true
  },
  "caps": {
    "company_usd": 0.02,
    "contact_usd": 0.03,
    "run_usd_max": 10.0,
    "max_domains_per_run": 20,
    "max_crawl_per_run": 20,
    "max_enrich_per_run": 20
  },
  "role_priority": {
    "ceo": 1,
    "founder": 1,
    "president": 1,
    "owner": 1,
    "managing director": 2,
    "vp": 2,
    "director": 3,
    "manager": 4,
    "default": 99
  },
  "exports": {
    "enabled": true
  },
  "default_country": "US",
  "exclusion_criteria": {
    "ownership_types": [],
    "max_locations": 10000,
    "excluded_keywords": []
  },
  "targeting": {
    "location_types": ["headquarters", "single_location"],
    "company_sizes": ["small", "mid-market"],
    "ownership_models": ["private"],
    "geographic_focus": ["North America", "United States"]
  },
  "intent_config": {
    "purpose": "digital_marketing",
    "target_action": "business_development",
    "intent_signals": {
      "high_intent": [
        "digital marketing",
        "SEO services",
        "web development",
        "marketing agency",
        "consulting services"
      ],
      "medium_intent": [
        "client services",
        "project work",
        "consulting"
      ],
      "low_intent": [
        "not accepting clients"
      ]
    },
    "intent_sources": [
      "about_pages",
      "team_pages",
      "contact_pages"
    ],
    "scoring_rules": {
      "high_intent_weight": 0.8,
      "medium_intent_weight": 0.4,
      "low_intent_weight": -0.3,
      "base_score": 0.5,
      "location_bonus": 0.1,
      "size_bonus": 0.1,
      "leadership_bonus": 0.1
    }
  }
}